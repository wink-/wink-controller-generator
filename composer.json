{
    "name": "wink/controller-generator",
    "description": "Generate production-ready Laravel controllers from database schemas and existing models",
    "type": "library",
    "keywords": [
        "laravel",
        "controller",
        "generator",
        "scaffolding",
        "crud",
        "api",
        "web"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Wink",
            "email": "support@wink.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0",
        "phpstan/phpstan": "^1.0",
        "nunomaduro/larastan": "^2.0",
        "laravel/pint": "^1.0",
        "vimeo/psalm": "^5.0",
        "psalm/plugin-laravel": "^2.0",
        "rector/rector": "^0.18",
        "infection/infection": "^0.27",
        "icanhazstring/composer-unused": "^0.8",
        "maglnet/composer-require-checker": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Wink\\ControllerGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wink\\ControllerGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wink\\ControllerGenerator\\ControllerGeneratorServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html build/coverage",
        "test:unit": "phpunit --testsuite=Unit",
        "test:feature": "phpunit --testsuite=Feature",
        "analyse": "phpstan analyse",
        "analyse:psalm": "psalm",
        "analyse:all": [
            "@analyse",
            "@analyse:psalm"
        ],
        "format": "pint",
        "format:test": "pint --test",
        "refactor": "rector",
        "refactor:dry": "rector --dry-run",
        "mutation": "infection",
        "mutation:ci": "infection --min-msi=80 --min-covered-msi=90",
        "security": "composer audit",
        "unused": "composer-unused",
        "check-deps": "composer-require-checker",
        "quality": [
            "@format:test",
            "@analyse:all",
            "@test",
            "@refactor:dry"
        ],
        "quality:fix": [
            "@format",
            "@refactor",
            "@test"
        ],
        "ci": [
            "@quality",
            "@security",
            "@unused",
            "@check-deps",
            "@mutation:ci"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}